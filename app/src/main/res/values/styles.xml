<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
	 <style name="collagechoice" parent="android:Widget.Holo.Light.TextView">
        <item name="android:background">@drawable/checked</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:clickable">true</item>
       
    </style>
    <style name="styleName" parent="android:Widget.Holo.Light.TextView">
        <item name="android:background">@drawable/cycle</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="RadioButt" parent="android:Widget.Holo.Light.CompoundButton.RadioButton">
        <item name="android:button">@null</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">20sp</item>
        <item name="android:padding">3dp</item>
        <item name="android:textColor">@drawable/xueyuan_text_selector</item>
    </style>
   
    
    
    
    <style name="per_student_insert_linear_style">
        
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/weiji</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:clickable">false</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:orientation">vertical</item>
    </style>
    
    <style name="per_student_insert_textview_style" parent="android:Widget.TextView">
        <item name="android:textColor">#8000</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    
       <style name="my_dialog_style" parent="@android:style/Theme.Holo.Dialog">
        	<item name="android:windowBackground">@color/holo</item>
       </style>
    
</resources>
